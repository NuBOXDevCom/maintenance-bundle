{
    "name": "nuboxdevcom/maintenance-bundle",
    "description": "Maintenance bundle for Symfony 3.x 4.x",
    "license": "MIT",
    "keywords": [
        "symfony",
        "bundle",
        "nubox",
        "nuboxdevcom",
        "maintenance",
        "maintenance-bundle"
    ],
    "type": "symfony-bundle",
    "homepage": "https://github.com/nuboxdevcom/maintenance-bundle",
    "support": {
        "issues": "https://github.com/nuboxdevcom/maintenance-bundle/issues",
        "source": "https://github.com/nuboxdevcom/maintenance-bundle",
        "docs": "https://github.com/nuboxdevcom/maintenance-bundle/blob/master/README.md"
    },
    "authors": [
        {
            "name": "NuBOX DevCom",
            "email": "slye@nubox.fr",
            "homepage": "https://github.com/nuboxdevcom",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/nuboxdevcom/maintenance-bundle"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/twig-bundle": "^3.4|^4.0",
        "symfony/templating": "^3.4|^4.0",
        "symfony/asset": "^3.4|^4.0",
        "symfony/yaml": "^3.4|^4.0",
        "twig/twig": "^3.4|^4.0"
    },
    "autoload": {
        "psr-4": {
            "NDC\\MaintenanceBundle\\": ""
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
