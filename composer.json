{
    "name": "nuboxdevcom/maintenance-bundle",
    "description": "Maintenance bundle for Symfony 3.x",
    "license": "MIT",
    "keywords": [
        "symfony",
        "bundle",
        "nubox",
        "nuboxdevcom",
        "maintenance",
        "maintenance-bundle"
    ],
    "type": "symfony-bundle",
    "homepage": "https://github.com/nuboxdevcom/maintenance-bundle",
    "support": {
        "issues": "https://github.com/nuboxdevcom/maintenance-bundle/issues",
        "source": "https://github.com/nuboxdevcom/maintenance-bundle",
        "docs": "https://github.com/nuboxdevcom/maintenance-bundle/blob/master/README.md"
    },
    "authors": [{
        "name": "NuBOX DevCom",
        "email": "slye@nubox.fr",
        "homepage": "https://github.com/nuboxdevcom",
        "role": "Developer"
    }],
    "repositories": [{
        "type": "git",
        "url": "https://github.com/nuboxdevcom/maintenance-bundle"
    }],
    "require": {
        "php": ">=7.0",
        "symfony/framework-bundle": "< 4.0",
        "twig/twig": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "NDC\\": "NDC/"
        }
    },
    "conflict": {
        "symfony/framework-bundle": "< 3.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
